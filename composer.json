{
    "name": "nstcactus/craft-translations-module",
    "description": "The simplest way to add administrable translations to a Craft CMS site",
    "type": "library",
    "require": {
        "nstcactus/craft-utils": "^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "nstcactus\\craftcms\\modules\\translations\\": "src/"
        }
    },
    "authors": [
        {
            "name": "nstcactus",
            "email": "nstcactus@gmail.com",
            "homepage": "https://github.com/nstcactus"
        }
    ],
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        }
    }
}
